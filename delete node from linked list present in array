class Solution:
    def modifiedList(self, nums: List[int], head: Optional[ListNode]) -> Optional[ListNode]:
        S=set(nums)
        dummy=ListNode(0,head)
        curr=dummy
        while curr.next:
            if curr.next.val in S:
                curr.next=curr.next.next
            else:
              curr=curr.next
        return dummy.next  
